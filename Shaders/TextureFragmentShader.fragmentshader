#version 400


in vec2 UV;
in vec4 newposition;
// Ouput data
out vec4 FragColor;

uniform sampler2D myTextureSampler;

// vec3 color = vec3(newposition);
vec3 color = vec3(0.95, 0.95, 0.54);

void main() {	

	FragColor = vec4(texture2D( myTextureSampler, UV ).rgb * color, 1.0);
	// FragColor = vec4(color, 1.0);
}